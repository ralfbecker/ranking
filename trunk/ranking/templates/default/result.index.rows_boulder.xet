					<vbox options="0,0">
						<hbox align="right" class="noPrint" options="0,0">
							<menulist>
								<menupopup type="select-number" class="noPrint noWrap" label="Top" onchange="check_tops(this)" id="set[$row_cont[PerId]][tops]" options=" ,1,8" statustext="Top: total number or no"/>
							</menulist>
							<menulist>
								<menupopup type="select-number" class="noPrint noWrap" label="Tries" onchange="check_top(this)" id="set[$row_cont[PerId]][top_tries]" options="No,1,30" statustext="Top: number of tries"/>
							</menulist>
						</hbox>
						<hbox align="right" class="noPrint" options="0,0">
