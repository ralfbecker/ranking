							</menulist>
							<menulist>
								<menupopup type="select-number" class="noPrint noWrap" label="Tries" id="set[$row_cont[PerId]][zone_tries]" options="No,1,30" statustext="Bonus: number of tries"/>
							</menulist>
						</hbox>
						<description no_lang="1" id="{$row}[result]" class="onlyPrint noWrap"/>
					</vbox>
