<?xml version="1.0"?>
<!-- $Id$ -->
<overlay>
	<template id="ranking.cup.list.rows" template="" lang="" group="0" version="1.9.001">
		<grid width="100%">
			<columns>
				<column/>
				<column width="50%"/>
				<column/>
				<column/>
				<column/>
				<column/>
			</columns>
			<rows>
				<row class="th">
					<nextmatch-sortheader align="center" label="Year" id="year"/>
					<nextmatch-sortheader label="Name" id="name"/>
					<nextmatch-filterheader align="center" id="nation" no_lang="1" options="Nation..."/>
					<nextmatch-sortheader align="center" label="Key" id="rkey"/>
					<nextmatch-sortheader label="Last modified" id="modified"/>
					<description align="center" value="Action"/>
				</row>
				<row class="row">
					<description align="center" id="${row}[year]" no_lang="1"/>
					<html id="${row}[name]" no_lang="1" class="ml10"/>
					<description align="center" id="${row}[nation]" no_lang="1"/>
					<description align="center" id="${row}[rkey]" no_lang="1"/>
					<vbox options="0,0">
						<date-time id="${row}[modified]" readonly="true"/>
						<menulist>
							<menupopup type="select-account" id="${row}[modifier]" readonly="true"/>
						</menulist>
					</vbox>
					<hbox align="center">
						<button statustext="View this cup" label="View" id="view[$row_cont[SerId]]" onclick="window.open(egw::link('/index.php','menuaction=ranking.ranking_cup_ui.view&amp;SerId=$row_cont[SerId]'),'_blank','dependent=yes,width=800,height=350,scrollbars=yes,status=yes'); return false;" image="view"/>
						<button statustext="Edit this cup" label="Edit" id="edit[$row_cont[SerId]]" onclick="window.open(egw::link('/index.php','menuaction=ranking.ranking_cup_ui.edit&amp;SerId=$row_cont[SerId]'),'_blank','dependent=yes,width=800,height=350,scrollbars=yes,status=yes'); return false;" image="edit"/>
						<button align="center" statustext="Delete this cup" label="Delete" id="delete[$row_cont[SerId]]" onclick="return confirm('Delete this cup');" image="delete"/>
					</hbox>
				</row>
			</rows>
		</grid>
		<styles>
			.ml10 { margin-left: 10px; }\r\n.nmh { font-weight: bold; }
		</styles>
	</template>
	<template id="ranking.cup.list" template="" lang="" group="0" version="1.3.001">
		<grid width="100%">
			<columns>
				<column/>
				<column/>
				<column/>
			</columns>
			<rows>
				<row disabled="!@msg">
					<description id="msg" no_lang="1" span="all" class="redItalic"/>
					<description/>
					<description/>
				</row>
				<row>
					<nextmatch align="center" id="nm" options="ranking.cup.list.rows" span="all"/>
				</row>
				<row>
					<button statustext="Add a cup" label="Add" id="nm[rows][edit][0]" onclick="window.open(egw::link('/index.php','menuaction=ranking.ranking_cup_ui.edit'),'_blank','dependent=yes,width=800,height=350,scrollbars=yes,status=yes'); return false;"/>
					<description/>
				</row>
			</rows>
		</grid>
	</template>
</overlay>