<?xml version="1.0"?>
<!-- $Id$ -->
<overlay>
	<template id="ranking.result.route" template="" lang="" group="0" version="1.3.004">
		<grid width="100%">
			<columns>
				<column/>
				<column/>
				<column/>
			</columns>
			<rows>
				<row disabled="!@msg">
					<description span="all" class="redItalic" align="center" id="msg" no_lang="1"/>
					<description/>
					<description/>
				</row>
				<row class="th" height="28">
					<description value="Competition"/>
					<menulist span="all">
						<menupopup id="WetId" onchange="1" no_lang="1" readonly="true"/>
					</menulist>
				</row>
				<row class="row">
					<description value="Category"/>
					<menulist>
						<menupopup id="GrpId" no_lang="1" readonly="true"/>
					</menulist>
					<hbox onchange="1" options="0,0" align="right">
						<menulist>
							<menupopup id="discipline" readonly="true"/>
						</menulist>
						<menulist class="leftPad5">
							<menupopup type="select-number" options=",4,6,1" id="route_num_problems" statustext="Number of problems"/>
						</menulist>
					</hbox>
				</row>
				<row class="row">
					<description value="Heat"/>
					<menulist>
						<menupopup id="route_order" no_lang="1" readonly="true"/>
					</menulist>
					<menulist class="noWrap">
						<menupopup id="route_type" align="right"/>
					</menulist>
				</row>
				<row class="th">
					<description value="Name"/>
					<textbox id="route_name" size="64" maxlength="80" span="all" needed="1" statustext="Qualification, 1/2-Final, ..."/>
				</row>
				<row class="row">
					<description value="Status"/>
					<hbox span="all" options="0,0">
						<menulist>
							<menupopup id="route_status"/>
						</menulist>
						<button id="button[startlist]" class="leftPad5" label="Generate Startlist"/>
					</hbox>
				</row>
				<row class="row" disabled="@route_order=-1">
					<description options="route_iso_open" value="Isolation"/>
					<hbox options="0,0" span="all">
						<textbox id="route_iso_open" size="20" maxlength="40" blur="opens"/>
						<textbox id="route_iso_close" size="20" maxlength="40" label="-" blur="closes"/>
					</hbox>
				</row>
				<row class="row" disabled="@route_order=-1">
					<description options=",,,route_start" value="Start heat"/>
					<textbox size="64" maxlength="64" id="route_start" blur="time" span="all"/>
				</row>
				<row class="row" disabled="@route_order=-1">
					<description options=",,,route_quota" value="Quota"/>
					<int id="route_quota"/>
					<description/>
				</row>
				<row class="row">
					<description value="Result official"/>
					<textbox size="64" maxlength="80" id="route_result" blur="time" span="all"/>
				</row>
				<row class="row">
					<description options=",,,route_judge" value="Judge"/>
					<textbox size="64" maxlength="80" id="route_judge" blur="name(s)" span="all"/>
				</row>
				<row class="row" valign="top">
					<description options=",,,route_comments" value="Comments"/>
					<textbox multiline="true" rows="8" cols="52" id="route_comments" span="all"/>
				</row>
				<row>
					<hbox span="2">
						<button label="Save" id="button[save]" statustext="Saves the changes made"/>
						<button id="button[apply]" label="Apply" statustext="Apply the changes made"/>
						<button id="button[cancel]" label="Cancel" statustext="Cannel without saving" onclick="window.close();"/>
						<html id="js"/>
					</hbox>
					<button id="button[delete]" label="Delete" align="right" statustext="Delete this heat including all results" onclick="return confirm('Delete this heat including all results');"/>
				</row>
			</rows>
		</grid>
	</template>
</overlay>